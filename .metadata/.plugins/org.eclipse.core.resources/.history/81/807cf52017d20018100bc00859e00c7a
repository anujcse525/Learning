package com.anuj.java8feature;

import java.util.Arrays;
import java.util.Comparator;
import java.util.HashSet;
import java.util.List;
import java.util.Set;

class Words{
	int index;
	String word;
	public int getIndex() {
		return index;
	}
	public void setIndex(int index) {
		this.index = index;
	}
	public String getWord() {
		return word;
	}
	public void setWord(String word) {
		this.word = word;
	}
	public Words(int index, String word) {
		super();
		this.index = index;
		this.word = word;
	}
	
}

public class FindAnagramSet {

	static class compStr implements Comparator<String>{

		@Override
		public int compare(String o1, String o2) {
			return o1.compareTo(o2);
		}
		
	}
	public static void main(String[] args) {
		FindAnagram findAna = (List<String> words) -> {
			Set<Set<String>> result = new HashSet<Set<String>>();
			Words[] wordsArr = new Words[words.size()];
			int i = 0;
			for(String word : words){
				wordsArr[i++] = new Words(i, word);
			}
			return null;
		};
		
	}
	
	public static List<String> sortStrings(List<String> words){
		List<String> list = Arrays.sort(words,new compStr());
	}
	
	

}
