package com.anuj.hackerrank;

import java.io.IOException;
import java.util.ArrayList;
import java.util.Arrays;
import java.util.List;
import java.util.Scanner;

public class FindGP {

	static long countTriplets(List<Long> list, long r) {
		long count = 0;
		long prevCount = count;
		Long[] arr = (Long[]) list.toArray();
		
		for (int i=0; i < arr.length; i++) {
			if (Arrays.binarySearch(arr, arr[i] * r) > 0 && Arrays.binarySearch(arr, arr[i] * r * r) > 0) {
				int index = Arrays.binarySearch(arr, arr[i] * r);
			}
		}
		return count;
	}

	public static void main(String[] args) throws IOException {
		Scanner scan = new Scanner(System.in);
		int items = scan.nextInt();
		int gp = scan.nextInt();
		List<Long> inputs = new ArrayList<Long>();

		for (int i = 0; i < items; i++) {
			inputs.add(scan.nextLong());
		}

		long result = countTriplets(inputs, gp);
		System.out.println(result);
		scan.close();
	}

}
