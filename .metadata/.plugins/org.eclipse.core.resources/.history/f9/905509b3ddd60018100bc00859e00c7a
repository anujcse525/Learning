package com.anuj.inmemory;

import java.util.LinkedHashMap;

class DataNode {
	String key;
	String value;
	DataNode prev;
	DataNode next;
	
	public DataNode(String key, String value) {
		this.key = key;
		this.value = value;
		this.prev = null;
		this.next = null;
	}

	@Override
	public String toString() {
		return "DataNode [key=" + key + ", value=" + value + ", prev=" + prev + ", next=" + next + "]";
	}
	
}

