package com.anuj.hackerrank;

import java.io.IOException;
import java.util.ArrayList;
import java.util.HashMap;
import java.util.HashSet;
import java.util.List;
import java.util.Map;
import java.util.Scanner;
import java.util.Set;

/*5 5
1 5 5 25 125*/
public class FindGP {

	static long countTriplets(List<Long> list, long r) {
		long count = 0;
		Map<Long, Long> map = new HashMap<Long, Long>();
		Set<Long> set = new HashSet(list);
		if(r ==1 ){
			int size = list.size();
			for(int i = size; i >= 3; i--)
				count += (i - 1)*(i -2) * (i  -3) /6;
			return count;
		}
		for (Long l : list) {
			if (map.containsKey(l)) {
				map.put(l, map.get(l) + 1);
			} else {
				map.put(l, 1L);
			}
		}

		for (Long l : set) {
			if (set.contains(l * r) && set.contains(l * r * r)) {
				count += map.get(l) * map.get(l * r) * map.get(l * r * r);
			}
		}
		
		return count;
	}

	public static void main(String[] args) throws IOException {
		Scanner scan = new Scanner(System.in);
		while (true) {
			int items = scan.nextInt();
			int gp = scan.nextInt();
			List<Long> inputs = new ArrayList<Long>();

			for (int i = 0; i < items; i++) {
				inputs.add(scan.nextLong());
			}

			long result = countTriplets(inputs, gp);
			System.out.println(result);
		}
		
	}

}
