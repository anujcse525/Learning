package com.anuj.inmemory;

import java.util.Arrays;
import java.util.Collections;
import java.util.Comparator;

class compare implements Comparator{

	@Override
	public int compare(Object o1, Object o2) {
		
		return ((String)o1).length() - ((String)o2).length();
	}
	
}
public class StringTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String abd = "The lines are here to sort.";
		String[] abdArr = abd.split(" ");
		Collections.sort(Arrays.asList(abdArr), new compare());
		int lenght = abd.length();
		System.out.println(abd.charAt(lenght - 1) +  " "+abd.charAt(lenght -1));
	}

	

}
