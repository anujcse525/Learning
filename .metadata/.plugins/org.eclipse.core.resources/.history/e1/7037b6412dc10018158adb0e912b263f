package com.accolite;

public enum Color {
	RED(20), GREEN(30), BLUE(40);
	int num;
	private Color(){
		
	}
	private Color(int abc) {
		System.out.println("Constructor");
		this.num = abc;
	}
	public static void main(String[] args) {
		Color cl = Color.BLUE;
		System.out.println(cl + ""+cl.num + Color.values());
	}
}
